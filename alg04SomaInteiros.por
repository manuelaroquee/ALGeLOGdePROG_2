programa {
  funcao inicio() {
    inteiro numero1
    inteiro numero2
    inteiro soma

    escreva("Digite o primeiro número inteiro: ")
    leia(numero1)
    escreva("Digite o segundo numéro inteiro: ")
    leia(numero2)

    soma = numero1 + numero2

    escreva("A soma entre " ,numero1, " e " ,numero2, " é igual a " ,soma )
  }
}
