programa { /* Crie um programa que leia o preço de um produto, calcule e mostre o seu
PREÇO PROMOCIONAL, com 5% de desconto. */
  funcao inicio() {
    real preco, precoProm

    escreva("Digite o valor do produto: ")
    leia(preco)

    precoProm = preco * 0.05

    escreva("O valor promocional deste produto é " ,precoProm)
    
  }
}
